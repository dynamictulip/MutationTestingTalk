{
  "$schema": "https://demotime.elio.dev/demo-time.schema.json",
  "title": "Mutation Test talk",
  "description": "",
  "version": 2,
  "demos": [
    {
      "title": "Start",
      "steps": [
        {
          "action": "closeAll"
        },
        {
          "action": "executeVSCodeCommand",
          "command": "demo-time.showPresenterView"
        }
      ],
      "icons": {
        "start": "run",
        "end": "pass-filled"
      }
    },
    {
      "title": "Intro slides",
      "description": "",
      "steps": [
        {
          "action": "snippet",
          "contentPath": ".demo/snippets/prep-for-slideshow.json"
        },
        {
          "action": "openSlide",
          "path": ".demo/slides/s-01-intro.md"
        }
      ],
      "notes": {
        "path": ".demo/notes/n-01-intro.md"
      },
      "icons": {
        "start": "vm",
        "end": "pass-filled"
      }
    },
    {
      "title": "Free look around",
      "description": "",
      "steps": [
        {
          "action": "snippet",
          "contentPath": ".demo/snippets/prep-for-code.json"
        },
        {
          "action": "executeVSCodeCommand",
          "command": "solutionExplorer.focus"
        }
      ],
      "notes": {
        "path": ".demo/notes/n-02.md"
      }
    },
    {
      "title": "Limitations of code coverage",
      "description": "",
      "steps": [
        {
          "action": "executeVSCodeCommand",
          "command": "workbench.view.testing.focus"
        }
      ]
    },
    {
      "title": "Add more tests to satisfy code coverage",
      "description": "",
      "steps": [
        {
          "action": "insert",
          "path": "Stuff.Tests/SuperImportantTests.cs",
          "position": 18,
          "contentPath": ".demo/stuff/IsLessThanFortyTwo",
          "lineInsertionDelay": 100
        }
      ],
      "notes": {
        "path": ".demo/notes/n-03.md"
      },
      "icons": {
        "start": "output-view-icon",
        "end": "pass-filled"
      }
    },
    {
      "title": "End",
      "description": "",
      "steps": [
        {
          "action": "snippet",
          "contentPath": ".demo/snippets/prep-for-slideshow.json"
        },
        {
          "action": "openSlide",
          "path": ".demo/slides/99-end.md"
        }
      ],
      "notes": {
        "path": ".demo/notes/n-99-end.md"
      },
      "icons": {
        "start": "vm",
        "end": "pass-filled"
      }
    },
    {
      "title": "Reset",
      "description": "",
      "steps": [
        {
          "action": "snippet",
          "contentPath": ".demo/snippets/prep-for-code.json"
        },
        {
          "action": "closeAll"
        }
      ],
      "icons": {
        "start": "close",
        "end": "pass-filled"
      }
    }
  ]
}
